    <DataSet Name="DeviceMaintenanceWindows">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ComputerName">
            <Value>=Parameters!ComputerName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>---- For tests in SQL mgmt studio only
--Declare @ComputerName as varchar(15) = 'server1';

Select SW.Description  
            ,SW.StartTime 
            ,[DurationInHours] = SW.Duration / 60  
            ,[SWType] = Case when SW.ServiceWindowType = 1 then 'All Deployments'
                        when SW.ServiceWindowType = 4 then 'Software Updates' 
                        when SW.ServiceWindowType = 5 then 'Task sequences' 
                        else 'Unknown' end 
from v_FullCollectionMembership FCM 
inner join v_ServiceWindow SW on FCM.CollectionID = SW.CollectionID
where FCM.ResourceID = (Select Distinct ResourceID from v_R_System where Name0 = @ComputerName)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StartTime">
          <DataField>StartTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="DurationInHours">
          <DataField>DurationInHours</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SWType">
          <DataField>SWType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
