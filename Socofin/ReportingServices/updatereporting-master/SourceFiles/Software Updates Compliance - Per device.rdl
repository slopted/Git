    <DataSet Name="DeviceUpdateCollections">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ComputerName">
            <Value>=Parameters!ComputerName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>---- For tests in SQL mgmt studio only
--Declare @ComputerName as varchar(15) = 'server1';

select CIA.CollectionName
from v_FullCollectionMembership FCM
inner join v_CIAssignment CIA on CIA.CollectionID = FCM.CollectionID
where CIA.AssignmentType in (1,5) and FCM.ResourceID = (select distinct ResourceID from v_R_System where Name0 = @ComputerName)
group by CIA.CollectionName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
