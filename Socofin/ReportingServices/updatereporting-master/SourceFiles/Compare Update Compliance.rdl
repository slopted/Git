<?xml version="1.0" encoding="utf-8"?>
<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <Description>Comparison of a maximum of six systems and their update compliance statuses</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="DataSource1">
      <DataSourceReference>/ConfigMgr_P11/{5C6358F2-4BB6-4a1b-A16E-8D96795D8602}</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>7f594453-7be9-46cf-aa6c-e4065a7931e2</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Compare">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@onlyRegUpdates">
            <Value>=Parameters!onlyRegUpdates.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Systems">
            <Value>=Parameters!Systems.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @onlyRegUpdates = 1

With SystemsLimited_CTE (ResourceID)
AS
(
	select top 6 ResourceID from v_R_System T00 where T00.ResourceID in (@Systems)

)

select SYS.Name0,
	concat(OS.caption0, ' ' ,OS.CSDVersion0) as OSystem,
	ui.Title,             
	Targeted=(case when ctm.ResourceID is not null then 'X' end), 
	Installed=(case when css.Status=3 then 'X' end), 
	IsRequired=(case when css.Status=2 then 'X' end)
	from v_Update_ComplianceStatus  css
	join v_R_System SYS on SYS.ResourceID = css.ResourceID 
	join v_UpdateInfo ui on ui.CI_ID=css.CI_ID 
	join v_CICategoryInfo_All vnd on vnd.CI_ID=ui.CI_ID and vnd.CategoryTypeName='Company'
	join v_CICategoryInfo_All cls on cls.CI_ID=ui.CI_ID and cls.CategoryTypeName='UpdateClassification'
	left join v_CITargetedMachines  ctm on ctm.CI_ID=css.CI_ID and ctm.ResourceID = css.ResourceID
	left join v_GS_OPERATING_SYSTEM OS on OS.ResourceID = CSS.ResourceID
where css.ResourceID in (select ResourceID from SystemsLimited_CTE)
and css.Status=2
order by SYS.name0

ELSE

With SystemsLimited_CTE (ResourceID)
AS
(
	select top 6 ResourceID from v_R_System T00 where T00.ResourceID in (@Systems)

)
select SYS.Name0,
	concat(OS.caption0, ' ' ,OS.CSDVersion0) as OSystem,
	ui.Title,            
	Targeted=(case when ctm.ResourceID is not null then 'X' end), 
	Installed=(case when css.Status=3 then 'X' end), 
	IsRequired=(case when css.Status=2 then 'X' end)
	from v_Update_ComplianceStatus  css
	join v_R_System SYS on SYS.ResourceID = css.ResourceID 
	join v_UpdateInfo ui on ui.CI_ID=css.CI_ID 
	join v_CICategoryInfo_All vnd on vnd.CI_ID=ui.CI_ID and vnd.CategoryTypeName='Company'
	join v_CICategoryInfo_All cls on cls.CI_ID=ui.CI_ID and cls.CategoryTypeName='UpdateClassification'
	left join v_CITargetedMachines  ctm on ctm.CI_ID=css.CI_ID and ctm.ResourceID = css.ResourceID
	left join v_GS_OPERATING_SYSTEM OS on OS.ResourceID = CSS.ResourceID
where css.ResourceID in (select ResourceID from SystemsLimited_CTE)
order by SYS.name0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
