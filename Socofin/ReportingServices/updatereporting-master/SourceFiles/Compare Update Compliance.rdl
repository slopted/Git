    </DataSet>
    <DataSet Name="Systems">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Search">
            <Value>=Parameters!Search.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select Name0, ResourceID
from v_R_System T00 
where T00.Client0 = 1 
and T00.Decommissioned0 !=1  
and T00.Obsolete0 !=1
and T00.Name0 like @Search
order by Name0</CommandText>
