        <Field Name="AssignmentName">
          <DataField>AssignmentName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Enabled">
          <DataField>Enabled</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
